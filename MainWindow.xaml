<Window x:Class="Hashmap.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hashmap"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="addbtn" Content="add" HorizontalAlignment="Left" Margin="88,218,0,0" VerticalAlignment="Top" Width="74" Click="addbtn_Click"/>
        <TextBox x:Name="txtkey" HorizontalAlignment="Left" Height="23" Margin="88,108,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtvalue" HorizontalAlignment="Left" Height="22" Margin="88,152,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="removebtn1" Content="remove" HorizontalAlignment="Left" Margin="88,250,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="showbtn" Content="show" HorizontalAlignment="Left" Margin="88,283,0,0" VerticalAlignment="Top" Width="75" Click="showbtn_Click"/>

    </Grid>
</Window>
